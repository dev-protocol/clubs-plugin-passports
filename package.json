{
	"name": "@devprotocol/clubs-plugin-passports",
	"version": "0.9.0-alpha.1",
	"type": "module",
	"description": "Passports plugin for clubs",
	"main": "dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/*.d.ts",
		"dist/*.js",
		"./*.d.ts",
		"!**/*.test.*",
		"!dist/vitest.*",
		"src"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./components": {
			"default": "./dist/components.js",
			"types": "./components.d.ts"
		},
		"./vue": {
			"default": "./dist/vue.js",
			"types": "./vue.d.ts"
		},
		"./svelte": {
			"default": "./dist/svelte.js",
			"types": "./svelte.d.ts"
		},
		"./utils": {
			"default": "./dist/utils.js",
			"types": "./utils.d.ts"
		},
		"./media": {
			"default": "./dist/media.js",
			"types": "./media.d.ts"
		},
		"./constants": {
			"default": "./dist/constants.js",
			"types": "./constants.d.ts"
		},
		"./types": {
			"default": "./dist/types.js",
			"types": "./types.d.ts"
		},
		"./package.json": "./package.json"
	},
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"test": "vitest",
		"preview": "astro --root .preview/preview.ssr dev",
		"preview:static": "astro --root .preview/preview.static dev",
		"build": "rollup -c",
		"prebuild": "rimraf dist ./*.d.ts",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --fix",
		"lint:format": "prettier --write '**/*.{ts,js,tsx,jsx,astro,vue,svelte,json,md,yml}'",
		"prepack": "yarn test --run && yarn prebuild && yarn build",
		"prepare": "husky",
		"typecheck": "astro check && tsc --noEmit && vue-tsc --noEmit --p tsconfig.vue.json"
	},
	"author": "Dev Protocol",
	"license": "PolyForm Perimeter License 1.0.0",
	"dependencies": {
		"@devprotocol/dev-kit": "8.7.0",
		"@devprotocol/util-ts": "4.0.0",
		"bignumber.js": "9.3.1",
		"dayjs": "^1.11.13",
		"nanoid": "^5.0.7",
		"ramda": "0.31.3",
		"redis": "^4.7.0",
		"sass": "1.90.0"
	},
	"peerDependencies": {
		"@devprotocol/clubs-core": "^3.18.0",
		"@devprotocol/clubs-plugin-payments": "^0.2.3 || ^0.3.0 || ^0.4.0",
		"ethers": "^6.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "0.9.4",
		"@astrojs/svelte": "7.1.0",
		"@astrojs/vue": "5.1.0",
		"@devprotocol/clubs-core": "3.27.0",
		"@devprotocol/clubs-plugin-payments": "0.5.0-alpha.7",
		"@eslint/js": "^9.14.0",
		"@rollup/plugin-typescript": "12.1.4",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.3",
		"@types/dotenv": "8.2.3",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/eslint__js": "^9.0.0",
		"@types/node": "22.17.2",
		"@types/ramda": "0.31.0",
		"@typescript-eslint/eslint-plugin": "8.40.0",
		"@typescript-eslint/parser": "8.40.0",
		"astro": "5.13.2",
		"dotenv": "17.2.1",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-functional": "^9.0.0",
		"ethers": "6.15.0",
		"globals": "^16.0.0",
		"husky": "9.1.7",
		"prettier": "3.6.2",
		"prettier-plugin-astro": "^0.14.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.6.0",
		"rimraf": "6.0.1",
		"rollup": "4.46.4",
		"rollup-plugin-dts": "^6.1.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.3",
		"typescript": "5.9.2",
		"typescript-eslint": "^8.13.0",
		"vitest": "^3.0.0",
		"vue": "3.5.18",
		"vue-tsc": "^3.0.0"
	},
	"packageManager": "yarn@4.9.2"
}
